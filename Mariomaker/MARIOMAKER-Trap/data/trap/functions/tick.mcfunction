# Hiddenhusk
## Sand
execute as @a[tag=playing] at @s if entity @s at @e[type=armor_stand,tag=hiddenhusksand,distance=..1.5] run summon husk ~ ~1 ~ {PersistenceRequired:1b,IsBaby:0}
execute as @a[tag=playing] at @s if entity @s at @e[type=armor_stand,tag=hiddenhusksand,distance=..1.5] run particle block sand ~ ~1 ~ 0 1 0 1 500
execute as @a[tag=playing] at @s if entity @s at @e[type=armor_stand,tag=hiddenhusksand,distance=..1.5] run playsound item.brush.brushing.sand.complete master @a ~ ~ ~ 5 1
execute as @a[tag=playing] at @s if entity @s at @e[type=armor_stand,tag=hiddenhusksand,distance=..1.5] run playsound entity.husk.hurt master @a ~ ~ ~ 1 1
execute as @a[tag=playing] at @s if entity @s at @e[type=armor_stand,tag=hiddenhusksand,distance=..1.5] run kill @e[type=armor_stand,tag=hiddenhusksand,sort=nearest,limit=1]

